{
  "compilerOptions": {
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "experimentalDecorators": true,
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "lib": ["es6", "dom"],
    "types": [ "webpack-env" ],
    "baseUrl": "./ClientApp/",
    "paths": {
        "Assets/*": [ "assets/*" ],
        //"ClientApp/*": [ "ClientApp/*" ],
        "Components/*": [ "components/*" ],
        "Constants/*": [ "constants/*" ],
        "Epics": [ "redux/epics" ],
        "Layouts/*": [ "components/layouts/*" ],
        "Reducers": [ "redux/reducers" ],
        "Reducers/*": [ "redux/reducers/*" ],
        "Redux/*": [ "redux/*" ],
        "Stores": [ "redux/stores" ],
        "Stores/*": [ "redux/stores/*" ],
        "Utils": [ "utils" ]
    }
  },
    "exclude": [
        "bin",
        "node_modules"
    ]
}
