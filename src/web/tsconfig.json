{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "experimentalDecorators": true,
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "lib": ["es6", "dom"],
    "types": [ "webpack-env" ],
    "paths": {
        "Assets/*": [ "ClientApp/assets/*" ],
        "ClientApp/*": [ "ClientApp/*" ],
        "Components/*": [ "ClientApp/components/*" ],
        "Constants/*": [ "ClientApp/constants/*" ],
        "Epics/*": [ "ClientApp/redux/epics/*" ],
        "Reducers/*": [ "ClientApp/redux/reducers/*" ],
        "Redux/*": [ "ClientApp/redux/*" ],
        "Stores/*": [ "ClientApp/redux/stores/*" ],
        "Utils/*": [ "ClientApp/utils/*" ]
    }
  },
    "exclude": [
        "bin",
        "node_modules"
    ]
}
